{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "pluginSearchDirs": [
    "./node_modules"
  ],
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-jsdoc"
  ]
}